{
    "name": "qbitartifacts/rec-api",
    "license": "Propietary",
    "type": "project",
    "description": "REC Barcelona API",
    "version": "2.29.0",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "alcaeus/mongo-php-adapter": "~1.1",
        "beberlei/doctrineextensions": "^1.2",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/mongodb-odm": "~1.0",
        "doctrine/mongodb-odm-bundle": "~3.0",
        "doctrine/orm": "^2.7",
        "egulias/email-validator": "^3.2",
        "friendsofsymfony/oauth-server-bundle": "^1.0",
        "friendsofsymfony/rest-bundle": "^2.0",
        "friendsofsymfony/user-bundle": "^2.0",
        "galbar/jsonpath": "^1.0",
        "h4cc/wkhtmltoimage-amd64": "~0.12",
        "h4cc/wkhtmltopdf-amd64": "~0.12",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/serializer-bundle": "^2.0",
        "knplabs/knp-snappy-bundle": "^1.6",
        "nelmio/cors-bundle": "~1.0",
        "ramsey/uuid": "~3.0",
        "sentry/sentry-symfony": "^3.0",
        "stephenhill/base58": "^1.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/browser-kit": "^4.4",
        "symfony/dotenv": "^5.4",
        "symfony/expression-language": "^4.4",
        "symfony/flex": "^1.19",
        "symfony/lock": "^4.4",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "~3.4.0",
        "symfony/polyfill-iconv": "~1.0",
        "symfony/serializer": "^4.4",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.8",
        "phpunit/phpunit": "^9.5",
        "symfony/debug-bundle": "^4.4",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "src-dir": "src/",
        "bin-dir": "bin/",
        "var-dir": "var/",
        "config-dir": "config/",
        "public-dir": "public/",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        },
        "branch-alias": null,
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
