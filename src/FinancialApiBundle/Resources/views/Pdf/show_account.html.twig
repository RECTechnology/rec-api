{% block show_account %}
    <strong>{% trans %}NAME{% endtrans %}:</strong> <em>{{ account.name }}</em><br />
    <strong>{% trans %}PHONE{% endtrans %}:</strong> <em>+{{ account.prefix }} {{ account.phone }}</em><br />
    <strong>{% trans %}EMAIL{% endtrans %}:</strong> <em>{{ account.email }}</em><br />
    <strong>{% trans %}ADRESS{% endtrans %}:</strong> <em>{{ account.street }} {{ account.addressNumber }} {{ account.country }} {{ account.city }} ({{ account.neighbourhood.name }}) {{ account.zip }}
{% endblock %}